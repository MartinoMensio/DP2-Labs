			<functionality>firewall</functionality>
		</node>
		<node name="WebServer1">
			<functionality>web_server</functionality>
		</node>
		<link name="link1">
			<src name="WebClient1" />
			<dst name="NAT1" />
		</link>
		<link name="link2">
			<src name="NAT1" />
			<dst name="Firewall1" />
		</link>
		<link name="link3">
			<src name="Firewall1" />
			<dst name="WebServer1" />
		</link>
		<link name="link4">
			<src name="NAT1" />
			<dst name="WebClient1" />
		</link>
		<link name="link5">
			<src name="Firewall1" />
			<dst name="NAT1" />
		</link>
		<link name="link6">
			<src name="WebServer1" />
			<dst name="Firewall1" />
		</link>
		<policy name="Policy1">
			<src name="WebClient1" />
			<dst name="WebServer1" />
			<result>true</result>
		</policy>
		<policy name="Policy2">
			<src name="WebServer1" />
			<dst name="WebClient1" />
			<result>true</result>
		</policy>
		<policy name="Policy3">
			<src name="WebClient1" />
			<dst name="WebServer1" />
			<result>true</result>
			<functionality>firewall</functionality>
		</policy>
	</nffg>
</nffgs>

<?xml version="1.0" encoding="UTF-8"?>
<nffgs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="nffgInfo.xsd">
	<nffg name="Example1">
		<node name="WebClient1">
			<functionality>web_client</functionality>
		</node>
		<node name="NAT1">
			<functionality>NAT</functionality>
		</node>
		<node name="Firewall1">
			<functionality>firewall</functionality>
		</node>
		<node name="WebServer1">
			<functionality>web_server</functionality>
		</node>
		<link name="link1">
			<src name="WebClient1" />
			<dst name="NAT1" />
		</link>
		<link name="link2">
			<src name="NAT1" />
			<dst name="Firewall1" />
		</link>
		<link name="link3">
			<src name="Firewall1" />
			<dst name="WebServer1" />
		</link>
		<policy name="Policy1">
			<src name="WebClient1" />
			<dst name="WebServer1" />
			<result>true</result>
		</policy>
		<policy name="Policy2">
			<src name="WebServer1" />
			<dst name="WebClient1" />
			<result>false</result>
		</policy>
		<policy name="Policy3">
			<src name="WebClient1" />
			<dst name="WebServer1" />
			<result>true</result>
			<functionality>firewall</functionality>
		</policy>
	</nffg>

	<nffg name="Example2">
		<node name="WebClient1">
			<functionality>web_client</functionality>
		</node>
